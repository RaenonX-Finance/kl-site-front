{
  "name": "kl-site-front",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prod:run": "pm2 start pm2.yml --env production",
    "prod:reload": "pm2 reload kl-site-front",
    "test": "echo 'jest --config jest.config.ts --maxWorkers=50% --colors'",
    "test:ci": "echo 'node --max-old-space-size=8192 ./node_modules/.bin/jest --config jest.config.ts --maxWorkers=50% --colors --ci --reporters=default --reporters=jest-junit --coverage'",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:css": "stylelint **/*.scss"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "bootstrap": "^5.1.3",
    "chroma-js": "^2.4.2",
    "lightweight-charts": "^3.8.0",
    "pm2": "^5.2.0",
    "react": "^17.0.2",
    "react-alert": "^7.0.3",
    "react-bootstrap": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "react-redux": "^8.0.2",
    "recharts": "^2.1.10",
    "typescript": "^4.5.5",
    "sass": "^1.49.9",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.3",
    "@types/node": "^16.11.22",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-alert": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "typescript": "^4.6.3",
    "stylelint": "^14.9.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-scss": "^4.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
